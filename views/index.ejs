
<!-- List of reviews below -->
<div class="container review">
    <h3>Check out our latest book reviews</h3>
    <% reviews.forEach((review, index) => { %>
       
        <li class="review-list"><a href="/book/review/<%=review.id%>" class="gb-links"><%= review.title %></a> by <%= review.author %>.</li>
        
    <% }); %> 
</div>

<!-- Fiction Container -->
<div class="container nyt-content mb-5">
    <div class="h4">The latest New York Times Fiction Best Sellers 
        <p class="h6">For week of <%= bestSellersDate %> </p>
    </div>
    <hr class="divider-line">
    
    <div class="nyt-box__container">
        <!--Container 1-->
        <div class="nyt-content__box">
            <p class="nyt-book__title"><%= book1Object.title %> </p>
            <p class="nyt-book__author">by <%= book1Object.author %> </p>
            <p class="nyt-book__rank">Rank: <%= book1Object.rank %> </p>
            <p class="nyt-book__isbn"><a href="https://www.google.com/search?tbo=p&tbm=bks&q=isbn:<%= book1Object.primary_isbn10 %>&num=10" target="_blank">Google ISBN Search</a></p> 
            <div class="nyt-content__box-content">
                <img src="<%= book1Object.book_image %>" alt="<%= book1Object.title %>" class="nyt-book--image">
               <p style="font-size: .9rem;"><%= book1Object.description %></p>
                
            </div>
        </div>          
